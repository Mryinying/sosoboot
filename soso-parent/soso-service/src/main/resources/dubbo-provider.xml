<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="soso-service"/>

    <dubbo:registry protocol="zookeeper" address="${dubbo.zookeeper}"/>

    <dubbo:protocol name="dubbo" port="20880"/>

    <dubbo:provider timeout="30000" retries="0"/>
    <!--<dubbo:provider timeout="30000" retries="0" filter="lock"/>-->

    <dubbo:service interface="com.it.service.UserService" ref="userService"/>
    <dubbo:service interface="com.it.service.CauseService" ref="causeService"/>
    
</beans>