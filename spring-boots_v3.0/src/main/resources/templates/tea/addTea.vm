#parse("./templates/layout/frame.vm")
				<div class="content col-lg-9">
					<form class="form-horizontal ele-info" action="/tea/insertTea" method="post">
					  <div class="form-group">
					    <label for="teaName" class="col-sm-2 control-label">姓名</label>
					    <div class="col-sm-10">
						    <input type="text" class="form-control" required="required" name="teaName" id="teaName" placeholder="姓名">
					    </div>
					  </div>
					  <div class="form-group">
					    <label for="teaSex" class="col-sm-2 control-label">性别</label>
					    <div class="col-sm-10">
						    <select class="form-control" name="teaSex">
							  <option value="1">男</option>
							  <option value="0">女</option>
							</select>
					    </div>
					  </div>
					  <div class="form-group">
					    <label for="teaNation" class="col-sm-2 control-label">民族</label>
					    <div class="col-sm-10">
						    <input type="text" class="form-control" required="required" name="teaNation" id="teaNation" placeholder="民族">
					    </div>
					    <input type="hidden" name="password" value="654321">
					  </div>
					  <button type="submit" class="btn btn-default">保存</button>
					</form>
				</div>
